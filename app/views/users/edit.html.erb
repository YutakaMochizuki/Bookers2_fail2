<h1>User info</h1>
<%= form_with model:@user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"" %>
  <label for "inputName">Image</label>
  <%= f.file_field :profile_image, placeholder: "", accept: "image/*" %>
  <label for "inputName">Introduction</label>
  <%= f.text_area :introduction, autofocus: true, id:"inputName", placeholder:"" %>
  <%= f.submit "Update User" %>
<% end %>